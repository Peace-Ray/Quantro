<?xml version="1.0" encoding="utf-8"?>
<resources>

	<!-- A file for storing all formatting placeholders and the "empty" fillers for them.
			These strings are used for placing specific information within display strings. -->
			
	
	
<!-- ||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||| -->
<!-- ||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||| -->
<!-- PLACEHOLDERS.  These strings appear in the various other strings, and are meant -->
<!--                to be replaced by specific data, provided at format time.        -->
	<!-- ||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||| -->
	<!-- ||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||| -->
	<!-- NAMES -->
	<!-- These strings are used as placeholders for a list of names - formatted and -->
	<!-- unformatted.  Generally, they can be applied to any list of strings, but we -->
	<!-- assume names will be used.  Specific formatting information may depend on -->
	<!-- context. -->
	
	<string name="placeholder_names_array_formatted">F_XXXX</string>		<!-- Formatted placement of an array of names -->
	<string name="placeholder_names_array_unformatted">U_XXXX</string>		<!-- Unformatted placement of an array of names -->
	<string name="placeholder_names_array_name_1">U1_XXXX</string>			<!-- The first name (string), index 0 in the provided array -->
	<string name="placeholder_names_array_name_2">U2_XXXX</string>			<!-- The second name (string), index 1 in the provided array -->

	<!-- ||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||| -->
	<!-- ||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||| -->
	<!-- SESSIONS -->
	<!-- These strings are used for session information -->
	<string name="placeholder_nonce">NONCE_XXXX</string>					<!-- Session nonce -->
	<string name="placeholder_lobby_nonce">NONCE_LOBBY_XXXX</string>					<!-- Lobby nonce -->
	

	<!-- ||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||| -->
	<!-- ||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||| -->
	<!-- GAMES -->
	<!-- These strings are used as placeholders for data concerning games and -->
	<!--  game modes. -->
	<string name="placeholder_game_mode_name">GM_N_XXXX</string>				<!-- name of the game mode, e.g. "Quantro Endurance" -->
	<string name="placeholder_game_mode_short_name">GM_SN_XXXX</string>				<!-- "short" name of the game mode, as defined by XML files and loaded through GameModes.  eg. "Retro" -->
	<string name="placeholder_game_mode_short_description">GM_SD_XXXX</string>		<!-- "short" description of the game mode, as defined by XML files and loaded through GameModes. -->
	<string name="placeholder_game_mode_class">GM_C_XXXX</string>		<!-- "short" description of the game mode, as defined by XML files and loaded through GameModes. -->
	<string name="placeholder_game_result_level">GR_L_XXXX</string>			<!-- Game level in a GameResult -->
	<string name="placeholder_game_result_level_previous">GR_LP_XXXX</string>		<!-- Game previous level in a GameResult -->
	<string name="placeholder_game_result_score">GR_S_XXXX</string>			<!-- Current score in a GameResult -->
	<string name="placeholder_game_result_rows">GR_R_XXXX</string>			<!-- Game rows in a GameResult -->
	<string name="placeholder_game_result_time">GR_T_XXXX</string>			<!-- Time passed in a GameResult -->
	<string name="placeholder_game_result_garbage">GR_G_XXXX</string>
	<string name="placeholder_game_result_first_level">GR_FL_XXXX</string>
	
	<!-- ||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||| -->
	<!-- ||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||| -->
	<!-- DURATIONS -->
	<!-- These strings are used as placeholders for data concerning duration, as -->
	<!-- defined in a long value of milliseconds.  These can be used with only the -->
	<!-- long, or related to another structure, such as the time until Challenge expiration. -->
	<!-- Durations may be positive or negative; negative durations are assumed to be in the past. -->
	<string name="placeholder_duration_date_near">DURATION_NEAR_DATE_XXXX</string>		<!-- Gives the near date after the duration expires; a "near" date is probably within 24 hours.  Probably this "date" is represented as a time.  The difference between this and a date is that we might preferentially prefer representations that offer limited information, such as only a time. -->
	<string name="placeholder_duration_hours">DURATION_HOURS_XXXX</string>			<!-- The number of hours (integer 0-MAX) -->
	<string name="placeholder_duration_minutes">DURATION_MINUTES_XXXX</string>		<!-- The number of minutes (integer 0-59) -->
	<string name="placeholder_duration_seconds">DURATION_SECONDS_XXXX</string>		<!-- The number of seconds (integer 0-59) -->


	<!-- ||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||| -->
	<!-- ||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||| -->
	<!-- LOBBIES -->
	<!-- These strings are used as placeholders for data concerning lobbies -->
	<!-- defined in a long value of milliseconds.  These can be used with only the -->
	<!-- long, or related to another structure, such as the time until Challenge expiration. -->
	<!-- Durations may be positive or negative; negative durations are assumed to be in the past. -->
	<string name="placeholder_lobby_name">LOBBY_NAME_XXXX</string>
	<string name="placeholder_lobby_description">LOBBY_DESCRIPTION_XXXX</string>
	<string name="placeholder_lobby_host_name">LOBBY_HOST_NAME_XXXX</string>
	<string name="placeholder_lobby_address">LOBBY_ADDRESS_XXXX</string>
	<string name="placeholder_lobby_hosted_people">LOBBY_HOSTED_PEOPLE_XXXX</string>
	<string name="placeholder_lobby_num_people">LOBBY_NUM_PEOPLE_XXXX</string>
	<string name="placeholder_lobby_max_people">LOBBY_MAX_PEOPLE_XXXX</string>
	<string name="placeholder_lobby_age_date_near">LOBBY_AGE_DATE_NEAR_XXXX</string>
	<string name="placeholder_lobby_inactive_player_name">LOBBY_INACTIVE_PLAYER_XXXX</string>		<!-- The first inactive player found (involved in a launch, or etc.) -->
	<string name="placeholder_lobby_other_players_first">LOBBY_OTHER_PLAYERS_F_XXXX</string>
	<string name="placeholder_lobby_other_players_formatted">LOBBY_OTHER_PLAYERS_F_XXXX</string>
	<string name="placeholder_lobby_other_players_unformatted">LOBBY_OTHER_PLAYERS_U_XXXX</string>
	<string name="placeholder_lobby_status_received_date_near">LOBBY_STATUS_RECEIVED_DATE_NEAR_XXXX</string>
	<string name="placeholder_lobby_name_null">-----</string>
	<string name="placeholder_lobby_host_name_null">-----</string>
	
