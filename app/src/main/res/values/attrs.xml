  <declare-styleable name="QuantroButton">
  	<!-- A QuantroTextButton is styleable; it extends QunatroWrappedContentButton, so you can use its attributes too. -->
  	<attr name="content_layout" format="reference"/>
  	<attr name="button_color" format="color"/>
  	<attr name="button_min_width" format="dimension"/>
  	<attr name="button_min_height" format="dimension"/>
  	<attr name="button_min_touchable_width" format="dimension"/>
  	<attr name="button_min_touchable_height" format="dimension"/>
  	<attr name="qb_style">
		    <enum name="plain" value="0" />
		    <enum name="block_underline" value="1" />
		    <enum name="block_three_tier_enclose_content" value="2" />
		    <enum name="block_three_tier_enclose_content_reverse" value="3" />
		    <enum name="block_two_tier_enclose_content" value="4" />
		    <enum name="block_two_tier_enclose_content_reverse" value="5" />
		    
		    <enum name="block_underline_two_tier_enclose_content" value="6" />
		    <enum name="block_underline_two_tier_enclose_content_reverse" value="7" />
		    
		    <enum name="block_two_tier_always_enclose_content" value="8" />
	</attr>
  </declare-styleable>
  
  <!-- A strip of QuantroContentWrappingButtons -->
  <declare-styleable name="QuantroButtonStrip">
  	<!-- The layout for buttons.  Optional, but if you use inflateButtons(), this must be defined. -->
  	<!-- Alternatively, one can provide buttons as children, or pass them in to addView. -->
  	<!-- If num_buttons is provided, we attempt to create up to that many buttons immediately upon construction. -->
  	<attr name="button_layout" format="reference"/>
  	<attr name="num_buttons" format="integer"/>
  	<attr name="button_style" format="integer"/>
  	<attr name="overflow">
		<enum name="none" value="0" />		<!-- Default: No overflow.  All buttons will be displayed. -->
		<enum name="allow" value="1" />		<!-- Overflow is allowed.  Only performed on API >= 11 (supporting PopupMenu) -->
		<enum name="force" value="2" />		<!-- Overflow is required.  On API < 11, buttons are dropped w/o displaying an "overflow" button. -->
	</attr>
  	<!-- Should we format the main button with the same width as the rest? -->
  	<attr name="main_button_equal_horizontal_weight" format="boolean"/>
  	<!-- Should we include main button trim?  What kind? -->
  	<attr name="trim">
		<enum name="none" value="0" />	
		<enum name="bounded" value="1" />		
		<enum name="runs_left" value="2" />		
		<enum name="runs_right" value="3" />		
	</attr>
  	<!-- Offsets, insets, borders, etc. -->
  	<attr name="button_border_width" format="dimension"/>
  	<attr name="button_border_width_bottom" format="dimension"/>
  	<attr name="button_border_width_top" format="dimension"/>
  	<attr name="button_gradient_height" format="dimension"/>
  	<attr name="button_horizontal_separation" format="dimension"/>
  	<attr name="main_button_horizontal_offset" format="dimension"/>
  	<attr name="sub_button_border_inset" format="dimension"/>
  	<attr name="sub_button_gradient_inset" format="dimension"/>
  	<attr name="sub_button_vertical_offset" format="dimension"/>
  	<attr name="main_button_content_margin_left" format="dimension"/>
  	<attr name="main_button_content_margin_top" format="dimension"/>
  	<attr name="main_button_content_margin_right" format="dimension"/>
  	<attr name="main_button_content_margin_bottom" format="dimension"/>
  	<attr name="sub_button_content_margin_left" format="dimension"/>
  	<attr name="sub_button_content_margin_top" format="dimension"/>
  	<attr name="sub_button_content_margin_right" format="dimension"/>
  	<attr name="sub_button_content_margin_bottom" format="dimension"/>
  	<attr name="sub_button_equal_width" format="boolean"/>
  	<attr name="button_bounds_width_min" format="dimension"/>
  	<attr name="button_bounds_width_max" format="dimension"/>
  	<attr name="button_bounds_height_min" format="dimension"/>
  	<attr name="button_bounds_height_max" format="dimension"/>
  </declare-styleable>
  
