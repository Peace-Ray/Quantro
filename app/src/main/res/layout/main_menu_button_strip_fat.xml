<?xml version="1.0" encoding="utf-8"?>
<!-- height WAS 55dp, now is wrap content. -->
<com.peaceray.quantro.view.button.strip.MainMenuButtonStrip
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:custom="http://schemas.android.com/apk/res/com.peaceray.quantro"
	android:id="@id/main_menu_button_strip"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	custom:button_border_width = "@dimen/button_medium_border_size"
	custom:button_gradient_height="0dip"
	custom:sub_button_border_inset="0dip"
	custom:main_button_horizontal_offset="0dip"
	custom:button_horizontal_separation="3dip"
	custom:button_bounds_height_min="@dimen/button_medium_size"
	android:paddingLeft="8dp"
	android:paddingRight="8dp"
	android:paddingBottom="4dp"
	android:paddingTop="4dp"
	>
  
	<com.peaceray.quantro.view.button.QuantroButton
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		custom:content_layout="@layout/content_title_big_description_center_alert_outer_gravity"
		android:clickable="true"/>

</com.peaceray.quantro.view.button.strip.MainMenuButtonStrip>
