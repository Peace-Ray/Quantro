<?xml version="1.0" encoding="utf-8"?>
<com.peaceray.quantro.view.button.strip.DialogButtonStrip
    android:id="@id/dialog_content_button_strip"
    xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:custom="http://schemas.android.com/apk/res-auto"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	custom:button_border_width = "4dip"
	custom:button_gradient_height="0dip"
	custom:sub_button_border_inset="0dip"
	custom:main_button_horizontal_offset="0dip"
	custom:button_horizontal_separation="4dip"
	custom:main_button_equal_horizontal_weight="true"
	>
  
  		<!-- Include three buttons  -->
   		<com.peaceray.quantro.view.button.QuantroButton
				 android:layout_width="wrap_content"
				 android:layout_height="wrap_content"
				 custom:content_layout="@layout/content_title_center_gravity_full_width"
				 android:clickable="true"/>
				 
		<com.peaceray.quantro.view.button.QuantroButton
				 android:layout_width="wrap_content"
				 android:layout_height="wrap_content"
				 custom:content_layout="@layout/content_title_center_gravity_full_width"
				 android:clickable="true"/>
				 
		<com.peaceray.quantro.view.button.QuantroButton
				 android:layout_width="wrap_content"
				 android:layout_height="wrap_content"
				 custom:content_layout="@layout/content_title_center_gravity_full_width"
				 android:clickable="true"/>
		
</com.peaceray.quantro.view.button.strip.DialogButtonStrip>

