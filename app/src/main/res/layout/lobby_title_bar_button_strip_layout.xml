<?xml version="1.0" encoding="utf-8"?>
<!-- height WAS 64dp -->
<com.peaceray.quantro.view.button.strip.LobbyTitleBarButtonStrip
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:custom="http://schemas.android.com/apk/res/com.peaceray.quantro"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	
	custom:button_layout="@layout/action_bar_image_button"
	
	custom:overflow="force"
	custom:trim="runs_left"
	
	custom:button_border_width = "4dip"
	custom:button_gradient_height="0dip"
	custom:sub_button_border_inset="0dip"
	custom:main_button_horizontal_offset="0dip"
	custom:button_horizontal_separation="3dip"
	android:paddingLeft="20dp"
	android:paddingRight="3dp"
	android:paddingBottom="4dp"
	android:paddingTop="4dp"
	>
  
  		<!-- Include three buttons  -->
   		<com.peaceray.quantro.view.button.QuantroButton
				 android:layout_width="wrap_content"
				 android:layout_height="wrap_content"
				 custom:content_layout="@layout/content_title_description_left_gravity"
				 android:clickable="false"/>
   		
</com.peaceray.quantro.view.button.strip.LobbyTitleBarButtonStrip>
