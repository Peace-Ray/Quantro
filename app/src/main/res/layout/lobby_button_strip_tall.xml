  custom:lbs_base_color_button_internet_itinerant_hide_open="@color/lobby_button_strip_open_hide"
  custom:lbs_base_color_button_internet_itinerant_hide_full="@color/lobby_button_strip_full_hide"
  custom:lbs_base_color_button_internet_itinerant_hide_closed="@color/lobby_button_strip_closed_hide"
  
  custom:lbs_image_drawable_button_wifi_hide_searching="@drawable/action_cancel_baked"
  custom:lbs_image_drawable_button_wifi_hide_open="@drawable/action_cancel_baked"
  custom:lbs_image_drawable_button_wifi_hide_full="@drawable/action_cancel_baked"
  custom:lbs_image_drawable_button_wifi_hide_closed="@drawable/action_cancel_baked"
  
  custom:lbs_image_drawable_button_internet_public_hide_open="@drawable/action_cancel_baked"
  custom:lbs_image_drawable_button_internet_public_hide_full="@drawable/action_cancel_baked"
  custom:lbs_image_drawable_button_internet_public_hide_closed="@drawable/action_cancel_baked"
  
  custom:lbs_image_drawable_button_internet_private_hide_open="@drawable/action_cancel_baked"
  custom:lbs_image_drawable_button_internet_private_hide_full="@drawable/action_cancel_baked"
  custom:lbs_image_drawable_button_internet_private_hide_closed="@drawable/action_cancel_baked"
  
  custom:lbs_image_drawable_button_internet_itinerant_hide_open="@drawable/action_cancel_baked"
