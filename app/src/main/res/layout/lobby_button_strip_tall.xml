  custom:lbs_image_drawable_button_internet_itinerant_hide_closed="@drawable/action_cancel_baked"
  
  
  
  custom:button_border_width="@dimen/button_medium_border_size"
  custom:button_gradient_height="0dip"
  custom:sub_button_border_inset="0dip"
  custom:main_button_horizontal_offset="0dip"
  custom:button_horizontal_separation="5dip"
  android:paddingLeft="8dp"
  android:paddingRight="8dp"
  android:paddingBottom="4dp"
  android:paddingTop="4dp"
  >
  
  		<!-- Include two buttons -->
   		<com.peaceray.quantro.view.button.QuantroButton
				 android:layout_width="wrap_content"
				 android:layout_height="wrap_content"
				 custom:content_layout="@layout/content_title_long_description_left"
				 android:clickable="true"/>
		<!-- For these two, previously used 
				custom:content_layout="@layout/content_title_description_center_gravity"
		 -->
		<com.peaceray.quantro.view.button.QuantroButton
				 android:layout_width="wrap_content"
				 android:layout_height="wrap_content"
				 custom:content_layout="@layout/content_image"
				 android:clickable="true"/>
</com.peaceray.quantro.view.button.strip.LobbyButtonStrip>
