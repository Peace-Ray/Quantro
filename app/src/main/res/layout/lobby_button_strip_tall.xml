<?xml version="1.0" encoding="utf-8"?>
<!-- Height WAS 55dp -->
<com.peaceray.quantro.view.button.strip.LobbyButtonStrip
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:custom="http://schemas.android.com/apk/res-auto"
  android:id="@id/lobby_button_strip"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
  
  custom:lbs_base_color_button_wifi_main_searching="@color/lobby_button_strip_searching_main"
  custom:lbs_base_color_button_wifi_main_open="@color/lobby_button_strip_open_main"
  custom:lbs_base_color_button_wifi_main_full="@color/lobby_button_strip_full_main"
  custom:lbs_base_color_button_wifi_hide_searching="@color/lobby_button_strip_searching_hide"
  custom:lbs_base_color_button_wifi_hide_open="@color/lobby_button_strip_open_hide"
  custom:lbs_base_color_button_wifi_hide_full="@color/lobby_button_strip_full_hide"
  
  custom:lbs_base_color_button_internet_public_main_open="@color/internet_lobby_button_strip_public_open_main"
  custom:lbs_base_color_button_internet_public_main_full="@color/internet_lobby_button_strip_public_full_main"
  custom:lbs_base_color_button_internet_public_main_closed="@color/internet_lobby_button_strip_public_closed_main"
  custom:lbs_base_color_button_internet_public_hide_open="@color/lobby_button_strip_open_hide"
  custom:lbs_base_color_button_internet_public_hide_full="@color/lobby_button_strip_full_hide"
  custom:lbs_base_color_button_internet_public_hide_closed="@color/lobby_button_strip_closed_hide"
  
  custom:lbs_base_color_button_internet_private_main_open="@color/internet_lobby_button_strip_private_open_main"
  custom:lbs_base_color_button_internet_private_main_full="@color/internet_lobby_button_strip_private_full_main"
  custom:lbs_base_color_button_internet_private_main_closed="@color/internet_lobby_button_strip_private_closed_main"
  custom:lbs_base_color_button_internet_private_hide_open="@color/lobby_button_strip_open_hide"
